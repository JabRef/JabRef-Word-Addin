    "@graphql-codegen/typescript-react-apollo": "2.2.5",
    "@types/find-process": "1.2.0",
    "@types/office-js": "^1.0.160",
    "@types/office-runtime": "^1.0.17",
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.8.4",
